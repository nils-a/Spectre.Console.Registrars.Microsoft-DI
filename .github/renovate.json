{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [ "github>nils-org/renovate-config" ],
  "packageRules": [
    {
      "matchPackageNames": ["cake.tool"],
      "enabled": false
    },
    {
      "matchPackagePatterns": [
        "^Spectre\\.Console.*",
        "^Microsoft.Extensions.DependencyInjection$"
      ],
      "bumpVersion": "minor"
    }
  ]
}
